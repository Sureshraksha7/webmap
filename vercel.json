{
  "version": 2,
  "routes": [
    {
      "src": "/(login|register|logout|profile|change-password|generate|structures.*)",
      "dest": "backend/api/app.py"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
