{
  "version": 2,
  "routes": [
    {
      "src": "/login",
      "dest": "api/app.py"
    },
    {
      "src": "/register",
      "dest": "api/app.py"
    },
    {
      "src": "/logout",
      "dest": "api/app.py"
    },
    {
      "src": "/profile",
      "dest": "api/app.py"
    },
    {
      "src": "/change-password",
      "dest": "api/app.py"
    },
    {
      "src": "/generate",
      "dest": "api/app.py"
    },
    {
      "src": "/structures(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "frontend/assets/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|html))",
      "dest": "frontend/$1"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}
