{
  "version": 2,
  "functions": {
    "backend/api/app.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/(login|register|logout|profile|change-password|generate|structures.*)", "dest": "/backend/api/app.py" },
    { "src": "/", "dest": "/frontend/index.html" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}